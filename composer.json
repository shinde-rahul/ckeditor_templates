{
  "name": "shinde-rahul/ckeditor_templates",
  "type": "drupal-module",
  "description": "Provides integration of the CKEditor IndentBlock plugin with the Drupal 8 CKEditor.",
  "keywords": ["Drupal"],
  "license": "GPL-2.0+",
  "homepage": "https://www.drupal.org/project/ckeditor_indentblock",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Christian Meilinger (meichr)",
      "homepage": "https://www.drupal.org/u/meichr",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "https://www.drupal.org/project/issues/ckeditor_indentblock",
    "source": "http://cgit.drupalcode.org/ckeditor_indentblock/"
  },
  "require": {
    "drupal/core": "^8.1.0",
    "drupal/ckeditor": "*",
    "drupal/libraries": "*",
    "ckeditor-plugin/templates": "4.11.4"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "ckeditor-plugin/templates",
        "version": "4.11.4",
        "type": "drupal-library",
        "vendor": "ckeditor-plugin",
        "dist": {
          "url": "https://download.ckeditor.com/templates/releases/templates_4.14.1.zip",
          "type": "zip"
        }
      }
    }
  ]
}

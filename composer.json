{
    "name": "shinde-rahul/ckeditor_templates",
    "description": "Integrates the CKEditor templates plugin.",
    "type": "drupal-module",
    "license": "GPL-2.0+",
    "keywords": ["Drupal"],
    "authors": [
        {
            "name": "Lucas Le Goff (lucaslg)",
            "homepage": "https://www.drupal.org/u/lucaslg",
            "role": "Maintainer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "drupal/core": "^8.1.0",
        "drupal/ckeditor": "*",
        "drupal/libraries": "*",
        "composer/installers": "~1.0",
        "ckeditor-plugin/ckeditor_templates": "*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/ckeditor_templates",
                "version": "4.14.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/templates/releases/templates_4.14.1.zip",
                    "type": "zip"
                }
            }
        }
    ]
}
